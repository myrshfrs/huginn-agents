# WebsiteAgent 

{
  "expected_update_period_in_days": "2",
  "url": "http://dz.china-embassy.org/chn/zytz/",
  "type": "html",
  "mode": "on_change",
  "extract": {
    "url": {
      "xpath": "//div[@class=\"nbox_ul\"]/ul/li/a",
      "value": "@href"
    },
    "title": {
      "xpath": "//div[@class=\"nbox_ul\"]/ul/li/a",
      "value": "string(.)"
    }
  }
}

